{
  "name": "terra-classic-linktree",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --config vite.config.js && vite build --ssr src/ssr.jsx --config vite.config.ssr.js",
    "start": "NODE_ENV=production node server.js",
    "preview": "vite preview",
    "serve": "NODE_ENV=production node server.js",
    "dev:ssr": "NODE_ENV=development node server.js",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "check": "tsc --noEmit && eslint src --ext ts,tsx"
  },
  "dependencies": {
    "compression": "^1.8.0",
    "express": "^4.18.2",
    "lucide-react": "^0.270.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.22.3",
    "sirv": "^3.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vite-plugin-node-polyfills": "^0.15.0",
    "vite-plugin-ssr": "^0.4.142"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.0.0",
    "postcss": "^8.0.0",
    "tailwindcss": "^3.0.0",
    "web-vitals": "^5.0.1"
  }
}
